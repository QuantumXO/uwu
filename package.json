  "scripts": {
    "start": "npm i && sed -i 's/git+ssh/git+https/g' package-lock.json && react-app-rewired start",
    "start:dev": "react-app-rewired start",
    "start:prod": "serve -s build",
    "build": "react-app-rewired build",
    "build:ipfs": "REACT_APP_IPFS_MODE=true npm run build",
    "test": "DOTENV_CONFIG_PATH='.env.local' cypress run --config watchForFileChanges=false numTestsKeptInMemory=0 --browser chrome",
    "test:main": "npx cypress-repeat run -n 2 --rerun-failed-only --spec \"cypress/integration/0-main-market/**/*.spec.ts\"",
    "test:amm": "npx cypress-repeat run -n 2 --rerun-failed-only --spec \"cypress/integration/1-amm-market/**/*.spec.ts\"",
    "test:polygon": "npx cypress-repeat run -n 2 --rerun-failed-only --spec \"cypress/integration/2-polygon-market/**/*.spec.ts\"",
    "test:avalanche": "npx cypress-repeat run -n 2 --rerun-failed-only --spec \"cypress/integration/3-avalanche-market/**/*.spec.ts\"",
    "test:ci": "start-server-and-test start:prod http://localhost:3000 test",
    "test:open": "DOTENV_CONFIG_PATH='.env.local' cypress open",
    "lint:fix": "prettier --write src",
    "gql-gen": "graphql-codegen --config codegen.yml",
    "analyze": "npx source-map-explorer 'build/static/js/*.js'"
  },
